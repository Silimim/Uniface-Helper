{
    "comments": {
        "lineComment": ";"
    },

    "brackets": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"]
    ],
    "indentationRules": {
        "increaseIndentPattern": "^((?!\\/\\/).)*(\\{[^}\"'`]*|\\([^)\"'`]*|\\[[^\\]\"'`]*)$",
        "decreaseIndentPattern": "^((?!.*?\\/\\*).*\\*/)?\\s*[\\)\\}\\]].*$"
    },
    "autoClosingPairs": [
        ["(", ")"],
        ["[", "]"],
        ["{", "}"],
        ["\"", "\""],
        ["'", "'"],
        ["`", "`"],
        ["if", " ()\n\nendif"],
        ["entry", "\nend"],
        ["operation", "\nend"],
    ],
    "surroundingPairs": [
        ["(", ")"],
        ["[", "]"],
        ["{", "}"],
        ["\"", "\""],
        ["'", "'"],
        ["`", "`"],
        ["if", " ()\n\nendif"],
        ["entry", "\nend"],
        ["operation", "\nend"],
    ]
}